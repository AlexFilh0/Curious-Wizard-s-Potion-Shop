<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/container_menu"
  android:layout_width="match_parent"
  android:layout_height="800dp"
  android:background="@drawable/background_roxo">

  <ImageView
    android:id="@+id/img_rectangle"
    android:layout_width="319.81dp"
    android:layout_height="466.66dp"
    android:layout_marginLeft="25.33dp"
    android:layout_marginTop="205.33dp"
    android:layout_gravity="top|left"
    android:background="@drawable/img_rectangle"
    android:scaleType="centerInside" />
  
  <ImageView
    android:id="@+id/img_nome"
    android:layout_width="190.76dp"
    android:layout_height="82.05dp"
    android:layout_marginLeft="82.67dp"
    android:layout_marginTop="237dp"
    android:layout_gravity="top|left"
    android:scaleType="centerInside"
    android:src="@drawable/image_nome" />
  
  <LinearLayout
    android:id="@+id/container_frame1"
    android:layout_width="126.91dp"
    android:layout_height="30.35dp"
    android:layout_marginLeft="124dp"
    android:layout_marginTop="450.67dp"
    android:layout_gravity="top|left"
    android:background="@drawable/container_frame1"
    android:clipToPadding="false"
    android:gravity="center"
    android:orientation="horizontal"
    android:paddingBottom="1.41dp"
    android:paddingLeft="3.33dp"
    android:paddingRight="3.33dp"
    android:paddingTop="1.41dp">
    
    <TextView
      android:id="@+id/txt_ranking"
      android:layout_width="wrap_content"
      android:layout_height="27.54dp"
      android:gravity="center_horizontal|center_vertical"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="RANKING"
      android:textColor="#FF000000"
      android:textSize="16dp" />
  </LinearLayout>
  
  <LinearLayout
    android:id="@+id/container_frame2"
    android:layout_width="126.91dp"
    android:layout_height="30.35dp"
    android:layout_marginLeft="124dp"
    android:layout_marginTop="493.33dp"
    android:layout_gravity="top|left"
    android:background="@drawable/container_frame2"
    android:clipToPadding="false"
    android:gravity="center"
    android:orientation="horizontal"
    android:paddingBottom="1.18dp"
    android:paddingLeft="3.33dp"
    android:paddingRight="3.33dp"
    android:paddingTop="1.18dp">
    
    <TextView
      android:id="@+id/txt_deslogar"
      android:layout_width="wrap_content"
      android:layout_height="27.98dp"
      android:gravity="center_horizontal|center_vertical"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="DESLOGAR"
      android:textColor="#FF000000"
      android:textSize="16dp" />
  </LinearLayout>
  
  <LinearLayout
    android:id="@+id/container_frame3"
    android:layout_width="126.91dp"
    android:layout_height="30.35dp"
    android:layout_marginLeft="124dp"
    android:layout_marginTop="534.67dp"
    android:layout_gravity="top|left"
    android:background="@drawable/container_frame2"
    android:clipToPadding="false"
    android:gravity="center"
    android:orientation="horizontal"
    android:paddingBottom="1.62dp"
    android:paddingLeft="3.33dp"
    android:paddingRight="3.33dp"
    android:paddingTop="1.62dp">
    
    <TextView
      android:id="@+id/txt_voltar"
      android:layout_width="wrap_content"
      android:layout_height="27.12dp"
      android:gravity="center_horizontal|center_vertical"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="VOLTAR"
      android:textColor="#FF000000"
      android:textSize="16dp" />
  </LinearLayout>
  
  <TextView
    android:id="@+id/txt_welcome"
    android:layout_width="wrap_content"
    android:layout_height="25.67dp"
    android:layout_marginLeft="123.33dp"
    android:layout_marginTop="372dp"
    android:layout_gravity="top|left"
    android:gravity="left|center_vertical"
    android:includeFontPadding="false"
    android:maxLines="1"
    android:singleLine="true"
    android:text="BEm VINDO, [login]"
    android:textColor="#FF000000"
    android:textSize="16dp" />
</FrameLayout>
